{
  "dataset": {
    "name": "glue",
    "source": "hf://glue/mnli",
    "subset_fraction": 1.0,
    "splits": {
      "train": 0.8,
      "val": 0.1,
      "test": 0.1
    },
    "features": {
      "text_a": "premise",
      "text_b": "hypothesis",
      "label": "label"
    }
  },
  "init": {
    "pretrained": "bert-base-uncased"
  },
  "training": {
    "loss": "CrossEntropyLoss",
    "optimizer": {
      "name": "AdamW",
      "lr": 2e-05,
      "weight_decay": 0.01
    },
    "scheduler": {
      "name": "linear",
      "kwargs": {
        "num_warmup_steps": 0
      }
    },
    "batch_size": 32,
    "epochs": 3,
    "metrics": [
      "accuracy"
    ]
  },
  "preprocessing": {
    "tokenizer": "bert-base-uncased",
    "max_len": 512,
    "augmentations": []
  },
  "nn_modules": {
    "BertForSequenceClassification": "transformers.AutoModelForSequenceClassification",
    "BertTokenizer": "transformers.AutoTokenizer",
    "CrossEntropyLoss": "torch.nn.CrossEntropyLoss",
    "AdamW": "torch.optim.AdamW",
    "LinearLR": "transformers.get_linear_schedule_with_warmup"
  }
}