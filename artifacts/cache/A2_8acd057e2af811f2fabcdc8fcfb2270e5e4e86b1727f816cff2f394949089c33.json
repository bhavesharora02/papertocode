{
  "dataset": {
    "name": "sst2",
    "source": "hf://glue/sst2",
    "subset_fraction": 1.0,
    "splits": {
      "train": 0.8,
      "val": 0.1,
      "test": 0.1
    },
    "features": {
      "text": "sentence",
      "label": "label"
    }
  },
  "init": {
    "pretrained": "bert-base-uncased"
  },
  "training": {
    "loss": "CrossEntropyLoss",
    "optimizer": {
      "name": "AdamW",
      "lr": 2e-05,
      "weight_decay": 0.01
    },
    "scheduler": {
      "name": "linear",
      "kwargs": {
        "num_warmup_steps": 0.1
      }
    },
    "batch_size": 32,
    "epochs": 3,
    "metrics": [
      "accuracy"
    ]
  },
  "preprocessing": {
    "tokenizer": "bert-base-uncased",
    "max_len": 128,
    "augmentations": []
  },
  "nn_modules": {
    "TransformerEncoder": "transformers.AutoModel",
    "CrossEntropyLoss": "torch.nn.CrossEntropyLoss"
  }
}