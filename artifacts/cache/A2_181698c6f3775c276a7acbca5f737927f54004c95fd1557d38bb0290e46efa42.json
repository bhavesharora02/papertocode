{
  "dataset": {
    "name": "GLUE MNLI",
    "source": "hf://glue/mnli",
    "subset_fraction": 1.0,
    "splits": {
      "train": "train",
      "val": "validation_matched",
      "test": "test_matched"
    },
    "features": {
      "text": [
        "premise",
        "hypothesis"
      ],
      "label": "label"
    }
  },
  "init": {
    "pretrained": "bert-base-uncased"
  },
  "training": {
    "loss": "CrossEntropyLoss",
    "optimizer": {
      "name": "AdamW",
      "lr": 2e-05,
      "weight_decay": 0.01
    },
    "scheduler": {
      "name": "linear",
      "kwargs": {
        "num_warmup_steps": 0
      }
    },
    "batch_size": 32,
    "epochs": 3,
    "metrics": [
      "accuracy"
    ],
    "target_metrics": {
      "accuracy": "SOTA on GLUE"
    }
  },
  "preprocessing": {
    "tokenizer": "bert-base-uncased",
    "max_len": 512,
    "augmentations": []
  },
  "nn_modules": {
    "BertForSequenceClassification": "transformers.BertForSequenceClassification",
    "BertTokenizer": "transformers.BertTokenizer",
    "CrossEntropyLoss": "torch.nn.CrossEntropyLoss",
    "AdamW": "torch.optim.AdamW",
    "get_linear_schedule_with_warmup": "transformers.get_linear_schedule_with_warmup"
  }
}