{
  "paper": {
    "title": "Deep Residual Learning for Image Recognition",
    "arxiv_id": "1512.03385",
    "tasks": [
      "Image Classification",
      "Object Detection",
      "Image Localization"
    ],
    "domain": "Computer Vision"
  },
  "dataset": {
    "name": "ImageNet 2012",
    "source": "hf://imagenet-1k",
    "subset_fraction": 1.0,
    "splits": {
      "train": 0.9,
      "val": 0.03,
      "test": 0.07
    },
    "features": {
      "text_a": "image",
      "text_b": "",
      "label": "class_label"
    }
  },
  "model": {
    "family": "ResNet",
    "variant": "ResNet-34",
    "framework": "torch",
    "layers": [
      {
        "type": "ConvStem",
        "params": {
          "params": {
            "in_channels": 3,
            "out_channels": 64,
            "kernel_size": 7,
            "stride": 2
          }
        }
      },
      {
        "type": "MaxPool",
        "params": {
          "params": {
            "kernel_size": 3,
            "stride": 2
          }
        }
      },
      {
        "type": "ResNetStage",
        "params": {
          "params": {
            "num_blocks": 3,
            "in_channels": 64,
            "out_channels": 64,
            "stride": 1
          }
        }
      },
      {
        "type": "ResNetStage",
        "params": {
          "params": {
            "num_blocks": 4,
            "in_channels": 64,
            "out_channels": 128,
            "stride": 2
          }
        }
      },
      {
        "type": "ResNetStage",
        "params": {
          "params": {
            "num_blocks": 6,
            "in_channels": 128,
            "out_channels": 256,
            "stride": 2
          }
        }
      },
      {
        "type": "ResNetStage",
        "params": {
          "params": {
            "num_blocks": 3,
            "in_channels": 256,
            "out_channels": 512,
            "stride": 2
          }
        }
      },
      {
        "type": "ClassificationHead",
        "params": {
          "params": {
            "in_features": 512,
            "num_classes": 1000,
            "pool_type": "average"
          }
        }
      }
    ],
    "init": {
      "pretrained": "from_scratch"
    },
    "num_labels": 1000
  },
  "training": {
    "loss": "CrossEntropyLoss",
    "optimizer": {
      "name": "SGD",
      "lr": 0.1,
      "kwargs": {
        "momentum": 0.9,
        "weight_decay": 0.0001
      }
    },
    "scheduler": {
      "name": "ReduceLROnPlateau",
      "kwargs": {
        "factor": 0.1
      }
    },
    "batch_size": 256,
    "epochs": 120,
    "metrics": [
      "top_1_accuracy",
      "top_5_accuracy"
    ],
    "target_metrics": {
      "ImageNet top-5 error": 7.46
    },
    "tolerance": 0.05
  },
  "preprocessing": {
    "tokenizer": "ImageNetPreprocessor",
    "max_len": 224
  }
}