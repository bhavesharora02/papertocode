{
  "paper": {
    "title": "Deep Residual Learning for Image Recognition",
    "arxiv_id": "1512.03385",
    "tasks": [
      "Image Classification",
      "Object Detection",
      "Image Localization",
      "Object Segmentation"
    ],
    "domain": "Image Recognition"
  },
  "dataset": {
    "name": "ImageNet 2012",
    "source": "hf://imagenet-1k",
    "subset_fraction": 1.0,
    "splits": {
      "train": 0.8,
      "val": 0.1,
      "test": 0.1
    },
    "features": {
      "text_a": "image",
      "text_b": "",
      "label": "label"
    }
  },
  "model": {
    "family": "ResNet",
    "variant": "ResNet-50",
    "framework": "torch",
    "layers": [
      {
        "type": "ConvBNReLU",
        "params": {
          "params": {
            "in_channels": 3,
            "out_channels": 64,
            "kernel_size": 7,
            "stride": 2,
            "padding": 3
          }
        }
      },
      {
        "type": "MaxPool2d",
        "params": {
          "params": {
            "kernel_size": 3,
            "stride": 2,
            "padding": 1
          }
        }
      },
      {
        "type": "ResidualStack",
        "params": {
          "params": {
            "num_blocks": 3,
            "block_type": "bottleneck",
            "in_channels": 64,
            "out_channels": 256,
            "stride": 1
          }
        }
      },
      {
        "type": "ResidualStack",
        "params": {
          "params": {
            "num_blocks": 4,
            "block_type": "bottleneck",
            "in_channels": 256,
            "out_channels": 512,
            "stride": 2
          }
        }
      },
      {
        "type": "ResidualStack",
        "params": {
          "params": {
            "num_blocks": 6,
            "block_type": "bottleneck",
            "in_channels": 512,
            "out_channels": 1024,
            "stride": 2
          }
        }
      },
      {
        "type": "ResidualStack",
        "params": {
          "params": {
            "num_blocks": 3,
            "block_type": "bottleneck",
            "in_channels": 1024,
            "out_channels": 2048,
            "stride": 2
          }
        }
      },
      {
        "type": "AdaptiveAvgPool2d",
        "params": {
          "params": {
            "output_size": [
              1,
              1
            ]
          }
        }
      },
      {
        "type": "Linear",
        "params": {
          "params": {
            "in_features": 2048,
            "out_features": 1000
          }
        }
      }
    ],
    "init": {
      "pretrained": "kaiming_he_initialization"
    },
    "num_labels": 1000
  },
  "training": {
    "loss": "CrossEntropyLoss",
    "optimizer": {
      "name": "SGD",
      "lr": 0.1
    },
    "scheduler": {
      "name": "ReduceLROnPlateau",
      "kwargs": {
        "factor": 0.1
      }
    },
    "batch_size": 256,
    "epochs": 120,
    "metrics": [
      "top1_accuracy",
      "top5_accuracy"
    ],
    "target_metrics": {
      "validation": {
        "top1_accuracy": 0.7715,
        "top5_accuracy": 0.9329
      }
    },
    "tolerance": 0.05
  },
  "preprocessing": {
    "tokenizer": "",
    "max_len": 128
  }
}